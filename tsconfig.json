{
	"compilerOptions": {
		"target": "ES6",
		"skipLibCheck": true,
		"moduleResolution": "bundler",
		"isolatedModules": true,
		"noEmit": true,
		"checkJs": true,
		"allowJs": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true
	},
	"include": ["src"]
}
